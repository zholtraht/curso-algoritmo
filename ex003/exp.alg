algoritmo "Exponencial"

var
  base: Real
  expo: Real

inicio
  Escreva("Digite um valor base: ")
    Leia(base)

  escreva("Digite um valor expoente: ")
    Leia(expo)

  Escreval("A exponenciacao informada foi: ", expo, " sobre ", base, ".")
  Escreva("O resultado da exponenciacao e: ", (Exp(base, expo)), ".")

fimalgoritmo
